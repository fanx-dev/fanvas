


using vaseGui

@Js
class DialogTest : BaseTestWin
{
  protected override Widget build() {
    VBox
    {
      Button {
        text = "Alert"
        it.onClick {
          AlertDialog("HI", "OK", "Cancel").show(root)
        }
      },
      Button {
        text = "Action"
        it.onClick {
          ActionDialog("HI", ["ABC", "1234", "5678"], "Cancel").show(root)
        }
      },
      Button {
        text = "Time"
        it.onClick {
          TimeDialog("OK", "Cancel").show(root)
        }
      },
      Button {
        text = "Date"
        it.onClick {
          DateDialog("OK", "Cancel").show(root)
        }
      },
    }
  }
}